{
  listen: { endpoints: ["tcp/localhost:7447"] },
  scouting: {
    multicast: { enabled: false },
    gossip: { enabled: false },
  },
  plugins: {
    rest: {
      __path__: ["~/Source/zenoh/target/release/libzenoh_plugin_rest.dylib"],
      http_port: 8000,
    },
    storage_manager: {
      __path__: [
        "~/Source/zenoh/target/release/libzenoh_plugin_storage_manager.dylib",
      ],
      storages: {
        issue_1052: {
          key_expr: "zenoh/issues/1052/*",
          volume: {
            id: "memory",
          },
          replica_config: {
            publication_interval: 20,
            propagation_delay: 200,
            delta: 1000,
          },
        },
      },
    },
  },
}
